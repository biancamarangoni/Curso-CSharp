@{
    var listaDeProf = ViewBag.ListaDeProfessores;
}

<h1>Listagem de Professores</h1>

<table class="table table-success table-striped">
    <thead>
    <th>Id</th>
    <th>Nome</th>
    <th>Email</th>
    <th>Endereço</th>
    <th>Telefone</th>
    <th>Cargo</th>
    <th>Ação</th>
    </thead>
    @foreach (var item in listaDeProf)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Nome</td>
            <td>@item.Email</td>
            <td>@item.Endereco</td>
            <td>@item.Telefone</td>
            <td>@item.Cargo</td>
            <td>
                <a href="https://localhost:44375/Professor/Editar/@item.Id" class="btn btn-info">Editar</a>
                <a href="https://localhost:44375/Professor/@item.Id" class="btn btn-danger">Excluir</a>
            </td>
        </tr>
    }

</table>
